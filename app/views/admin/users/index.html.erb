<%= render "admin/home/header" %>

<div class="container">
	<div class="row">
		<div class="col-sm-8 offset-sm-2">

			<div class="row">
				<h3>会員一覧</h3>
			</div>

			<table>
				<tr>
					<th>id</th>
					<th>email</th>
					<th></th>
				</tr>

				<% @users.each do |u| %>
				<tr>
					<td><%= u.id %></td>
					<td><%= link_to u.email, admin_user_path(u.id) %></td>
					<td></td>
				</tr>
				<% end %>

			</table>

		</div>
	</div>
</div>